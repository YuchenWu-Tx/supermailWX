/* components/tabBar/tanBar.wxss */
.tab-control-wrap {
  width:100%;
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  text-align: center;
  background-color: #fff;
}
.tab-control-wrap	.tab-control-item {
  flex: 1;
}
.tab-control-wrap	.active{
  color:#666;
}
.tab-control-wrap	.active span {
  border-bottom: 4rpx solid #ff8198;
  padding: 10rpx;
}