.index_swiper .swiper {
  height: 340rpx;
}
.index_swiper .swiper image {
  width: 100%;
}
.nav_meun {
  display: flex;
}
.nav_meun .meun_item {
  flex: 1;
  padding: 20rpx;
}
.nav_meun .meun_item .meun_img {
  width: 100%;
}
.floor_group .floor_title {
  padding-top: 20rpx;
  display: flex;
  align-content: flex-end;
}
.floor_group .floor_list {
  overflow: hidden;
}
.floor_group .floor_list .floor_item {
  box-sizing: border-box;
  width: 33.33333333vw;
  padding: 10rpx;
  float: left;
}
.floor_group .floor_list .floor_item:nth-last-child(-n+4) {
  height: 27.72711207vw;
}
.floor_group .floor_list .floor_item:nth-child(-n+1) {
  height: 55.45422414vw;
}
.floor_group .floor_list .floor_item image {
  width: 100%;
  height: 100%;
}
